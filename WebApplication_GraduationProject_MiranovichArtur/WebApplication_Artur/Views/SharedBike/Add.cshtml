@model AddSharedBikeViewModel

<form action="/SharedBike/Add?" method="post">

    <div class="addBlock">

        <input class="pooof" name="BikeId" value=@Model.BikeId />
        <input class="pooof" name="Id" value=@Model.Id) />


        <div class="addOneSharedBike">
            @Html.EditorFor(x => x.BikeSize)  - рвзмер рамы.
        </div>

        <div class="addOneSharedBike">
            @Html.EditorFor(x => x.RearChainrings)  - количество задних звёзд.
        </div>

        <div class="addOneSharedBike">
            @Html.EditorFor(x => x.FrontChainrings)  - количество передних звёзд.
        </div>

        <div class="addOneSharedBike">
            @Html.EditorFor(x => x.WheelDiametr)  - диаметр колёс.
        </div>

        <div class="addOneSharedBike">
            @Html.EditorFor(x => x.BrakeDiametr)  - диаметр тормозов.
        </div>

        <div class="addOneSharedBike">
            <input type="submit" value="применить" class="inputaddBlock" />
        </div>

    </div>



</form>
