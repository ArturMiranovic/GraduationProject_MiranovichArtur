@model List<GalereyBikeViewModel>

<div class="nameForFiltr">

    ссылка на фильтры

</div>

<div class="galerey-all-bike">

    @foreach (var bike in Model)
    {
        <div class="invisibleFrame">

            <a href="/Bike/PageBike?idBike=@bike.Id">

                <div class="galerey-one-bike">

                    <div>
                        @bike.Name
                    </div>

                    <div class="imageBikeAll">
                        <img src="@bike.Page" class="logoBikeGalerey"/>
                    </div>

                    <div>
                        @bike.BikeClass велосипед. Размер рамы: @bike.BikeSize'.
                    </div>

                    <div>
                        @bike.Price
                    </div>
                </div>

            </a>
        </div>
    }

</div>