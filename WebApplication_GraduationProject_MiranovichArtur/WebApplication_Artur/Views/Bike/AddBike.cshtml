@model AddBikeViewModel


<form action="/Bike/AddBike" method="post">
    <div>

        <div>
            Введдите модель велосипеда:
        </div>

        <div>
            @Html.TextBoxFor(x => x.Name)
        </div>

    </div>

    <div>

        <div>
            Укажите цену:
        </div>

        <div>
            @Html.TextBoxFor(x => x.Price)
        </div>

    </div>

        <div class="inutRegistration">
            <input type="submit" value="Выставить" />
        </div>


</form>